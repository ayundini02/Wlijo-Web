// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./wlijo.db"
}

generator client {
  provider = "prisma-client-js"
}

model produk {
  idProduk Int @id
  jenisProduk String
  namaProduk String
  gambarProduk String
  hargaProduk Int
  deskripsi String
}
